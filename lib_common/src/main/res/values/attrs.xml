<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--  标题组合控件  -->
    <declare-styleable name="TitleBar">
        <!-- 背景 -->
        <attr name="tb_background" format="color"/>
        <!-- 左边图标 -->
        <attr name="tb_left_image" format="reference|color"/>
        <!-- 右边图标 -->
        <attr name="tb_right_image" format="reference|color"/>
        <!-- 标题文字 -->
        <attr name="tb_title" format="string"/>
        <!-- 标题文字颜色 -->
        <attr name="tb_title_color" format="color"/>
        <!-- 标题文字大小 -->
        <attr name="tb_title_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SubmitView">
        <attr name="sv_bindEditText1" format="reference" />
        <attr name="sv_bindEditText2" format="reference" />
        <attr name="sv_bindEditText3" format="reference" />
    </declare-styleable>

</resources>